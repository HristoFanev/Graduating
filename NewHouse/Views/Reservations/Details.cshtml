            Номер на жилище
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Properties.Id)
        </dd>
        <dt class = "col-sm-2">
           @* @Html.DisplayNameFor(model => model.Users)*@
           Клиент
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Users.Id)
        </dd>
